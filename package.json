{
  "name": "duplicate-requests",
  "version": "1.1.2",
  "description": "Express middleware to avoid duplicates requests made by client",
  "keywords": [
    "express",
    "middleware",
    "requests"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "jest tests/",
    "test:coverage": "jest tests/ --coverage && cd coverage/lcov-report && live-server",
    "test:integration": "jest integration/",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint src/",
    "prepublishOnly": "tsc"
  },
  "files": ["build/"],
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/romch007/duplicate-requests.git"
  },
  "author": "Romain Chardiny <romain.chardiny@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/romch007/duplicate-requests/issues"
  },
  "homepage": "https://gitlab.com/romch007/duplicate-requests#readme",
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.4",
    "@types/keyv": "^3.1.1",
    "@types/node": "^14.0.13",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "jest": "^26.1.0",
    "live-server": "^1.2.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "keyv": "^4.0.1",
    "@types/express": "^4.17.7"
  },
  "optionalDependencies": {
    "@keyv/mongo": "^1.1.0",
    "@keyv/redis": "^2.1.2"
  },
  "engines": { "node": ">=6.0.0" }
}
